<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>task1</servlet-name>
        <servlet-class>task1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uranai</servlet-name>
        <servlet-class>uranai</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FortuneTelling</servlet-name>
        <servlet-class>org.camp.servlet.FortuneTelling</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResultData</servlet-name>
        <servlet-class>org.camp.servlet.ResultData</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>task1</servlet-name>
        <url-pattern>/task1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uranai</servlet-name>
        <url-pattern>/uranai</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FortuneTelling</servlet-name>
        <url-pattern>/FortuneTelling</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResultData</servlet-name>
        <url-pattern>/ResultData</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
